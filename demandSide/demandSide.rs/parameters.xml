<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="qualityScale" displayName="Number of Quality decimals to consider" type="int" 
						defaultValue="4" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="stopAt" displayName="Stop at" type="double" 
						defaultValue="100.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="initiallyKnownByPerc" displayName="Percentage of Consumers that know the firm at entry" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="minWelfareParam" displayName="Minimum of Welfare Parameter" type="double" 
						defaultValue="0.1" 
						isReadOnly="true" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="firmsEntryOnlyAtStart" displayName="Firms Entry only at Start?" type="boolean" 
						defaultValue="true" 
