		/>
				<parameter name="richestProbability" displayName="Probability of addressing Richest Consumer" type="double" 
						defaultValue="0.99" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="gini" displayName="Gini Index" type="double" 
						defaultValue="0.7" 
						isReadOnly="true" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
